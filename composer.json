{
    "name": "montefuscolo/xmpp",
    "description": "Library for XMPP protocol (Jabber) connections",
    "license": "BSD-2-Clause",
    "homepage": "https://github.com/fabiang/xmpp",
    "keywords": ["jabber", "xmpp"],
    "authors": [
        {
            "name": "Fabian Grutschus",
            "email": "f.grutschus@lubyte.de",
            "homepage": "http://www.lubyte.de/",
            "role": "developer"
        }
    ],
    "autoload": {"psr-0": {"Fabiang\\Xmpp\\": "src/"}},
    "require": {
        "php": ">=5.3",
        "psr/log": "*"
    },
    "require-dev": {
        "monolog/monolog": "*",
        "phpunit/phpunit": "~3.7",
        "behat/behat": "2.5.*@stable"
    },
    "suggest": {
        "monolog/monolog": "Allows more advanced logging of the application flow"
    },
    "config": {
        "bin-dir": "bin/"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "archive": {
        "exclude": [
            ".gitignore",
            ".gitattributes",
            ".scrutinizer.yml",
            ".travis.yml",
            "/tests",
            "/docs",
            "/bin",
            "/example.php"
        ]
    }
}
